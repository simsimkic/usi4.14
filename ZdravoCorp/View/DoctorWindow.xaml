<Window x:Class="ZdravoCorp.View.DoctorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Loaded="DoctorWindow_OnLoaded"
        Title="DoctorWindow" Height="450" Width="500" ResizeMode="NoResize">
    <Grid>
        <Button Content="Appointments" HorizontalAlignment="Left" Margin="39,50,0,0" VerticalAlignment="Top" Height="39" Width="80" Click="OpenAppointmentView" ClickMode="Press"/>
        <Button Content="Patients" HorizontalAlignment="Left" Height="40" Margin="39,113,0,0" VerticalAlignment="Top" Width="80" Click="OpenPatientView" ClickMode="Press"/>
        <Button Content="Notifications" HorizontalAlignment="Left" Height="40" Margin="39,176,0,0" VerticalAlignment="Top" Width="80" Click="OpenNotificationView" ClickMode="Press"/>
        <Button Click="Logout" Content="Logout" HorizontalAlignment="Left" Height="48" Margin="361,353,0,0" VerticalAlignment="Top" Width="102"/>

    </Grid>
</Window>
