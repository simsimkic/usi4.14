<Window x:Class="ZdravoCorp.View.ManagerMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="ManagerMainWindow" Height="200" Width="300">

    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_Equipment">
                <MenuItem Header="_Table" Name="EquipmentTableBtn" Click="EquipmentTableBtn_Click"></MenuItem>
                <MenuItem Header="_RequestTable" Name="RequestEquipmentTableBtn" Click="ReqstEquipmentTableBtn_Click"></MenuItem>
                <MenuItem Header="_EquipmentTransfer" Name="EquipmentTransferBtn" Click="EquipmentTransferBtn_Click"></MenuItem>
            </MenuItem>
        </Menu>
        <Button Content="Logout" HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="20" Width="50" Margin="5,5,5,5" Click="Button_Click"></Button>
    </DockPanel>
   
</Window>
