<Window x:Class="ZdravoCorp.View.DoctorAddMedicalReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorp.View"
        mc:Ignorable="d"
        Title="DoctorAddMedicalReport" Height="350" Width="800">
    <Grid>
        <Label Content="Medical Report" HorizontalAlignment="Center" Height="38" Margin="0,33,0,0" VerticalAlignment="Top" Width="156" FontSize="20"/>
        <TextBox Name="DiagnosisTextBox" HorizontalAlignment="Left" Height="120" Margin="137,114,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="490" FontSize="14"/>
        <Label Content="Diagnosis:" HorizontalAlignment="Left" Height="40" Margin="32,114,0,0" VerticalAlignment="Top" Width="105" FontSize="20"/>
        <Button x:Name="EndMedicalReportButton" Content="End Report" HorizontalAlignment="Left" Height="41" Margin="641,250,0,0" VerticalAlignment="Top" Width="127" FontSize="20" Click="EndMedicalReport"/>
    </Grid>
</Window>
